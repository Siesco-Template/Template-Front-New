{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"strict": true,
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"allowJs": true,
		"checkJs": false,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["svgr.d.ts","src"],
	"exclude": ["node_modules", "dist"]
  }
  